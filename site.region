				if (key == branch) {
					return [];
				}
				return $('<a>').addClass('nav-link').attr({
					'href':'#',
				}).html(branches[key]).on('click', (e) => {
					e.preventDefault();
					setCookie('release-branch', key, 365, undefined, true);
					location.reload();
				});
			})
		)
	)
